{
    "name": "myapp/project",
    "description": "PHP Backend Project",
    "type": "project",
    "require": {
        "php": "^8.2",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
